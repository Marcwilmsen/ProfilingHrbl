					"editorMode": "code",
					"format": "table",
					"rawQuery": true,
					"rawSql": "WITH BoxStops AS (\n    SELECT\n        o.country,\n        p.box_number,\n        COUNT(p.id) AS stops\n    FROM orders o\n    JOIN order_boxes ob ON o.order_number = ob.order_id\n    JOIN pickdata_entries p ON ob.box_number = p.box_number\n    GROUP BY o.country, p.box_number\n)\nSELECT\n    country,\n    AVG(stops) AS average_stops_per_box\nFROM BoxStops\nGROUP BY country;",
